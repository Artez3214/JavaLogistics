<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="loginpages" prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.kursinis.fxControllers.LoginPage">
   <children>
      <TextField fx:id="username" layoutX="76.0" layoutY="140.0" promptText="Username" />
      <PasswordField fx:id="password" layoutX="76.0" layoutY="180.0" promptText="Password" />
      <Button layoutX="26.0" layoutY="250.0" mnemonicParsing="false" onAction="#validate" prefHeight="18.0" prefWidth="100.0" text="Log in" />
      <Button layoutX="180.0" layoutY="250.0" mnemonicParsing="false" onAction="#register" prefHeight="15.0" prefWidth="90.0" text="Sign up" />
   </children>
</AnchorPane>
